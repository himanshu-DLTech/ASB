{
    "flow": {
        "name":"XML to JSON for Docx",
        "disabled": true,
        "expandRouteProperties": true
    },
    "listener": {
        "type":"custom_file", 
        "isMessageGenerator": true,
        "path":"{{{ESB_DIR}}}/translateDocProcessing/unZipedDoc/*",
        "donePath":"{{{ESB_DIR}}}/translateDocProcessing/unZipedDocForJson"
    },
    "route0":{
        "type": "js",
        "dependencies":["listener"],
        "isAsync": false,
        "module":"{{{ESB_DIR}}}/custom/xmlToJson.js"
    }
    
}

